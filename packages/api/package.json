{
  "name": "@vybit/api-sdk",
  "version": "1.1.0",
  "description": "Developer API SDK for Vybit notification platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf dist *.tsbuildinfo",
    "dev": "tsc --watch",
    "test": "jest --config ../../jest.config.js --rootDir ../..",
    "lint": "echo 'Run lint from root: npm run lint'"
  },
  "keywords": [
    "vybit",
    "api",
    "rest",
    "sdk",
    "notifications",
    "developer-api"
  ],
  "author": "Vybit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/flatirontek/vybit-sdk.git",
    "directory": "packages/api"
  },
  "homepage": "https://gitlab.com/flatirontek/vybit-sdk#readme",
  "bugs": {
    "url": "https://gitlab.com/flatirontek/vybit-sdk/-/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@vybit/core": "^1.1.0"
  },
  "peerDependencies": {
    "node-fetch": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "node-fetch": {
      "optional": true
    }
